
(rules PCB ESP32 Project Make an IoT Gadget
  (snap_angle 
    fortyfive_degree
  )
  (autoroute_settings
    (fanout off)
    (autoroute on)
    (postroute on)
    (vias on)
    (via_costs 50)
    (plane_via_costs 5)
    (start_ripup_costs 100)
    (start_pass_no 2457)
    (layer_rule F.Cu
      (active on)
      (preferred_direction horizontal)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 2.8)
    )
    (layer_rule B.Cu
      (active on)
      (preferred_direction vertical)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 1.5)
    )
  )
  (rule
    (width 250.0)
    (clear 200.2)
    (clear 125.0 (type smd_to_turn_gap))
    (clear 50.0 (type smd_smd))
  )
  (padstack "Via[0-1]_800:400_um"
    (shape
      (circle F.Cu 800.0 0.0 0.0)
    )
    (shape
      (circle B.Cu 800.0 0.0 0.0)
    )
    (attach off)
  )
  (via 
    "Via[0-1]_800:400_um" "Via[0-1]_800:400_um" default
  )
  (via 
    "Via[0-1]_800:400_um-kicad_default" "Via[0-1]_800:400_um" "kicad_default"
  )
  (via_rule
    default "Via[0-1]_800:400_um"
  )
  (via_rule
    "kicad_default" "Via[0-1]_800:400_um-kicad_default"
  )
  (class default
    (clearance_class default)
    (via_rule default)
    (rule
      (width 250.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class "kicad_default"
    "/TOUCH_IRQ" /MISO /MOSI "/TOUCH_CS" /SCK /VCC "/TFT_DC" "/TFT_RESET"
    "/TFT_CS" "/SD_CS" /SDA /SCL /VIN /IO34 /IO35 /IO32
    /IO33 /IO25 /IO26 /IO27 /IO14 /IO12 /IO17 /IO16
    "Net-(JP1-Pad2)" GNDPWR "Net-(D1-Pad1)" "Net-(JP2-Pad2)" "Net-(D3-Pad1)" "Net-(JP3-Pad2)" "Net-(D2-Pad1)" "Net-(D4-Pad1)"
    /EN "/SENSOR_VP" "/SENSOR_VN" /IO13 /SD2 /SD3 /CMD /TXD0
    /RXD0 /SD1 /SD0 /CLK /VBAT
    (clearance_class "kicad_default")
    (via_rule kicad_default)
    (rule
      (width 250.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
)